{
  "name": "ai-evaluation-system",
  "version": "1.0.0",
  "description": "Enhanced AI system for autonomous vehicle and robotics evaluation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.json",
    "lint:fix": "eslint . --ext .js,.json --fix",
    "build": "echo 'Build completed'",
    "docker:build": "docker build -t ai-evaluation-system .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix"
  },
  "keywords": [
    "ai",
    "evaluation",
    "autonomous-vehicles",
    "robotics",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "socket.io": "^4.7.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^9.33.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
